{
  "name": "v-element",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint:es": "eslint src/**/*.{vue,ts}",
    "spellcheck": "cspell lint --dot --cache --gitignore --color --show-suggestions \"src/**/*.@(html|js|cjs|mjs|ts|tsx|css|scss|md|vue)\"",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "commitizen": "4.3.1",
    "commitlint": "20.2.0",
    "cspell": "9.4.0",
    "cz-git": "1.12.0",
    "eslint": "9.39.1",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-vue": "10.6.2",
    "stylelint": "16.26.1",
    "stylelint-config-recommended": "^17.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0",
    "vite": "7.2.4",
    "vue-tsc": "3.1.4"
  }
}
