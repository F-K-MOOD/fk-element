{
  "name": "@xuqizhang/fk-element",
  "version": "1.0.0",
  "description": "基于 Vue 3 的组件库",
  "type": "module",
  "author": "frank Mood",
  "license": "MIT",
  "keywords": [
    "Vue3",
    "component",
    "library"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangyifan007/v-element.git"
  },
  "private": true,

  "files": [
    "dist"
  ],
  "sideEffects": ["dist/fk-element.css"],
  "main": "dist/umd/fk-element.umd.cjs",
  "module": "dist/es/fk-element.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/fk-element.es.js",
      "require": "./dist/umd/fk-element.umd.cjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "prepare": "husky",
    "dev": "vite",
    "build": "vite build",
    "build:es": "vite build --config vite.es.config.ts",
    "build:umd": "vite build --config vite.umd.config.ts",
    "build:all": "pnpm run build && pnpm run build:es && pnpm run build:umd",
    "prepublishOnly": "pnpm run build:all",
    "preview": "vite preview",
    "move-style": "move-file dist/es/fk-element.css dist/fk-element.css",
    "lint:es": "eslint src/**/*.{vue,ts}",
    "lint:style": "stylelint src/**/*.vue",
    "spellcheck": "cspell lint --dot --cache --gitignore --color --show-suggestions \"src/**/*.@(html|js|cjs|mjs|ts|tsx|css|scss|md|vue)\"",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.7.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "commitizen": "4.3.1",
    "commitlint": "20.2.0",
    "cspell": "9.4.0",
    "cz-git": "1.12.0",
    "eslint": "9.7.0",
    "eslint-plugin-simple-import-sort": "12.1.0",
    "eslint-plugin-vue": "9.27.0",
    "jsdom": "^27.3.0",
    "move-file-cli": "^3.0.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-nested": "^7.0.2",
    "postcss-scss": "^4.0.9",
    "postcss-styled-syntax": "^0.7.1",
    "sass": "^1.95.1",
    "stylelint": "16.23.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended": "17.0.0",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-scss": "^6.13.0",
    "typescript": "5.9.3",
    "typescript-eslint": "7.16.0",
    "vite": "7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16",
    "vue-tsc": "3.1.4",
    "vue": "^3.5.24"
  }
}